{
  "name": "two-women-in-trouble",
  "version": "0.1.1",
  "description": "Two Women in Trouble",
  "main": "electron-main.js",
  "keywords": [
    "game",
    "vue.js",
    "snowpack"
  ],
  "author": "Mia Cain",
  "license": "MIT",
  "scripts": {
    "start": "snowpack dev",
    "build": "cross-env NODE_ENV=production snowpack build",
